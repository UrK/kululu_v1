<!DOCTYPE>
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:fb="https://www.facebook.com/2008/fbml"  xml:lang="en" lang="en">
<head runat="server">
    <title>@ViewBag.Title</title>
    <link rel="icon" href="@Url.Content("~/Content/img/favicon.ico")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/reset.css")"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/kululu_" + PageData["dirStr"] + ".css")"/>
    <!--[if lt IE 9]>
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/kululu-ie.css")"/>
    <![endif]-->
    @RenderSection("CSS", false)
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.js" type="text/javascript"></script>
    @RenderSection("Scripts", false)
</head> 

<body>
    <div id="fb-root"></div>
    <div id="main" class="FBContainer clearfix">
        @RenderBody()
        @Html.Partial(MVC.Shared.Views._FooterSlim)
        @RenderSection("ScriptsEND", false)
    </div>
</body>
</html>