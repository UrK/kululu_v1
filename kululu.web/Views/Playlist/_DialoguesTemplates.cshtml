<script type="text/x-jqote-template" id="templateConnect"><![CDATA[
    <div class="dialogue">
        <h1> @Kululu.Web.Views.Playlist.Playlist.templateConnect_Title </h1>
        <p>  @Kululu.Web.Views.Playlist.Playlist.templateConnect_Connect  </p>
        <div class="actions">
            <a class="ui-btnSubmit">
                @Kululu.Web.Views.Playlist.Playlist.templateConnect_Login
            </a>
            <a class="ui-btnCancel">
                @Kululu.Web.Views.Playlist.Playlist.templateConnect_Cancel
            </a>
        </div>
    </div>
]]></script>
<script type="text/x-jqote-template" id="DialogComments"><![CDATA[
    <div class="dialogue">
        <ul class="comments">
            <* $(this.comments).each(function(){ *>
            <li class="group" data-id="<*=this.id*>">
                <div class="img">
                    <a href="https://www.facebook.com/profile.php?id=<*=this.from.id*>" target="_blank">
                        <img src="https://graph.facebook.com/<*=this.from.id*>/picture"/>
                    </a>
                </div>
                <div class="bd">
                    <a href="https://www.facebook.com/profile.php?id=<*=this.from.id*>" class="from" target="_blank">
                        <*=this.from.name*>
                    </a>
                    <span class="message"><*=this.message*></span>
                    <div class="footer">
                        <abbr class="timeago"><*=this.created_time*></abbr>
                        <* if (typeof(this.likes) != 'undefined') { *>
                        <a><*=this.likes*> @Kululu.Web.Views.Playlist.Playlist.DialogComments_likes </a>
                        <* }; *>
                    </div>
                </div>
            </li>
            <* }); *>
            <li class="new group">
                <div class="img">
                    <a href="https://www.facebook.com/profile.php?id=<*=Globals.User.FBID*>" target="_blank">
                        <img src="https://graph.facebook.com/<*=Globals.User.FBID*>/picture"/>
                    </a>
                </div>
                <div class="bd">
                    <textarea class="elastic defaultInput" data-postid="<*=this.postid*>">@Kululu.Web.Views.Playlist.Playlist.DialogComments_YourOpinion</textarea>
                </div>
            </li>
        </ul>
        <div class="actions">
            <a class="ui-btnCancel">סגור</a>
        </div>
    </div>
]]></script>
<script type="text/x-jqote-template" id="DialogFeedback"><![CDATA[
    <div class="dialogue">
