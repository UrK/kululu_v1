        <p> @Kululu.Web.Views.Playlist.Playlist.DialogFeedback_Subtitle</p>
        <div class="inputWrapper">
            <textarea id="comment" name="comment" class="" spellcheck="false" tabindex="0" rows="10" cols="50">@Kululu.Web.Views.Playlist.Playlist.DialogFeedback_Content</textarea>
        </div>
        <div class="actions">
            <a class="ui-btnSubmit" tabindex="0" id="sendFeedback" href="javascript:void(0);">@Kululu.Web.Views.Playlist.Playlist.DialogFeedback_Send</a>
            <a class="ui-btnCancel" tabindex="0" >@Kululu.Web.Views.Playlist.Playlist.DialogFeedback_AnotherTime</a>
        </div>
    </div>
]]></script>

