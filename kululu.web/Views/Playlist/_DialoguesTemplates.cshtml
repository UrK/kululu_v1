<script type="text/x-jqote-template" id="templateHarvestDialogue"><![CDATA[
    <div class="dialogue">
        <h2>@Kululu.Web.Views.Playlist.Playlist.TemplateHarvestPreDialogue_Title</h2>

        <span class="import-title"> <*= this.importTitle *> </span> 
        
        <span class="<* if (!this.likeImportPerformed) { *>hidden" <* } else { *>" <*}; *>>
            <span class="import-likesComplete"> <*= this.importLikesComplete*>  / <*= this.importLikesTotal *>  </span>
        </span>

        <div class="<* if (this.complete) { *>hidden" <* } else { *>" <*}; *>>
            <img src="@Url.Content("~/Content/img/ajax-loader2.gif")" title="@Kululu.Web.App_GlobalResources.Strings.loadingTitle"/>
        </div>

        <div class="actions" >
            <* if (this.complete) { *>
            <a class="ui-btnCancel">@Kululu.Web.Views.Shared.Shared.close</a>
            <* } *>
        </div>
    </div>
]]></script>


<script type="text/x-jqote-template" id="templateVotesDialogue"><![CDATA[
<div class="dialogue">
    <* if (this.votesPositive.length > 0) { *>
    <h2 class="love"><img src="@Url.Content("~/Content/img/img_trans.gif")"/> @Kululu.Web.Views.Playlist.Playlist.templateVotesDialogue_LoveTheSong </h2>
    <ul class="votes clearfix">
        <* $(this.votesPositive).each(function() { *>
        <li>
            <a href="https://www.facebook.com/profile.php?id=<*=this.id*>" target="_blank" class="img">
                <img src="<*=this.photo*>"/>
            </a>
        </li>
        <* }); *>
    </ul>
    <* }; *>
    <* if (this.votesNegative.length > 0) { *>
    <h2 class="hate"><img src="@Url.Content("~/Content/img/img_trans.gif")"/> @Kululu.Web.Views.Playlist.Playlist.templateVotesDialogue_HateTheSong </h2>
    <ul class="votes clearfix">
        <* $(this.votesNegative).each(function(){ *>
        <li>
            <a href="https://www.facebook.com/profile.php?id=<*=this.id*>" target="_blank" class="img">
                <img src="<*=this.photo*>"/>
            </a>
        </li>
        <* }); *>
    </ul>
    <* }; *>

    <div class="actions">
        <a class="ui-btnCancel">@Kululu.Web.Views.Shared.Shared.close</a>
    </div>
</div>
]]></script>
<script type="text/x-jqote-template" id="DialogueTemplate_EditSong"><![CDATA[
<div class="editSongDetails dialogue" data-images=<*=JSON.stringify(this.images)*> data-imageindx="1">
    <h1> @Kululu.Web.Views.Playlist.Playlist.DialogueTemplateEditSong_Title</h1>
    <div class="media group">
        <div class="img">
            <img src="<*=this.thumbnail*>"/>
            <input type="hidden" name="thumbnail" value="<*=this.thumbnail*>" class="thumbnail"/>
            <a class="btnChangeImage">@Kululu.Web.Views.Playlist.Playlist.DialogueTemplateEditSong_SwitchPicture</a>
        </div>
        <div class="bd">
            <fieldset>
                <label>@Kululu.Web.Views.Playlist.Playlist.DialogueTemplateEditSong_ArtistName</label>
                <div class="inputWrapper">
                    <input type="text" value="<*=this.artist*>" name="artist" class="artist <*=this.language*>"/>
                </div>
            </fieldset>
            <fieldset>
                <label>@Kululu.Web.Views.Playlist.Playlist.DialogueTemplateEditSong_SongName</label>
                <div class="inputWrapper">
                    <input type="text" value="<*=this.title*>" name="title" class="title <*=this.language*>"/>
                </div>
            </fieldset>
        </div>
    </div>
    <div class="actions clearfix">
        <button class="btnSaveDetails ui-btnSubmit" role="button">@Kululu.Web.Views.Playlist.Playlist.save</button>
        <a class="btnCancelEditDetails ui-btnCancel" role="button">@Kululu.Web.Views.Playlist.Playlist.cancel</a>
    </div>
</div>
</script>
